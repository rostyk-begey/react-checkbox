{
  "name": "react-checkbox-hook",
  "version": "1.0.7",
  "description": "A React hook to easily work with checkboxes",
  "keywords": [
    "frontend",
    "react",
    "react-hook",
    "hook",
    "checkbox"
  ],
  "author": "Rostyslav Behei <rostyk.begey@gmail.com>",
  "license": "MIT",
  "repository": "rostyk-begey/react-checkbox-hook",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "test": "cross-env CI=1 jest",
    "test:watch": "jest --watch",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "yarn run build && husky install",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cross-env": "^5.2.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^2.0.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "react": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^2.26.9",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "eslint --cache"
  },
  "collectCoverageFrom": [
    "**/*.{ts,tsx}",
    "src/**/*.{ts,tsx}",
    "!**/*.d.ts",
    "!**/node_modules/**"
  ]
}
